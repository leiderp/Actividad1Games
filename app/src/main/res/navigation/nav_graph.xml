<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/menu">

    <fragment
        android:id="@+id/menu"
        android:name="com.leiderp.MemoryGame.menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_menu_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_menu_to_game2"
            app:destination="@id/game2" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.leiderp.MemoryGame.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/game2"
        android:name="com.leiderp.MemoryGame.game2"
        android:label="activity_game2"
        tools:layout="@layout/activity_game2" />
</navigation>